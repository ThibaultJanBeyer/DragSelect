{
  "name": "dragselect",
  "version": "3.0.0",
  "description": "Easy JavaScript library for selecting and moving elements. With no dependencies. Drag-Select & Drag-And-Drop.",
  "main": "./dist/DragSelect.js",
  "types": "./dist/DragSelect.d.ts",
  "scripts": {
    "open": "open __tests__/quicktest.html -a 'Google Chrome'",
    "dev": "pnpm open & pnpm watch",
    "watch": "concurrently --kill-others \"pnpm rollup:watch\" \"pnpm test:watch\"",
    "\n# TESTING:": "",
    "jest": "jest --detectOpenHandles",
    "test": "pnpm build && pnpm jest",
    "test:watch": "pnpm jest --watch",
    "\n# BUILDING:": "",
    "build": "pnpm rollup",
    "rollup": "rollup -c",
    "rollup:watch": "pnpm rollup -w"
  },
